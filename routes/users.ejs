                    <!-- Content Row -->
                    <div class="row">

                        <div class="col-xl-10 card-body">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>No</th>
                                        <th>User ID</th>
                                        <th>Email</th>
                                        <th>Name</th>
                                        <th>Role</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% rows.forEach((item, index) => { %>
                                        <tr>
                                            <td><%= index + 1 %></td>
                                            <td><%= item.userid %></td>
                                            <td><%= item.email %></td>
                                            <td><%= item.name %></td>
                                            <td><%= item.role %></td>
                                            <td></td>
                                        </tr>
                                    <% }) %>
                                </tbody>
                            </table>
                           

                        </div>

                      
                    </div>

                </div>
                <!-- /.container-fluid -->


            <!-- Footer -->
