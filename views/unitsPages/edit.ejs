    <!-- Sidebar Toggler (Sidebar) -->
    <div class="text-center d-none d-md-inline">
        <button class="rounded-circle border-0" id="sidebarToggle"></button>
    </div>
    </ul>
    <!-- End of Sidebar -->
    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">
        <!-- Main Content -->
        <div id="content">
            <%- include('../partials/topbar') %>
                <!-- Begin Page Content -->
                <div class="container-fluid">
                    <!-- Page Heading -->
                    <h1 class="h3 mb-2 text-gray-800">Users</h1>

                    <!-- DataTales Example -->
                    <form action="" method="post">
                        <div class="card shadow mb-4">
                            <div class="card-header py-3">
                                <h4 class="m-0 font-weight-bold text-primary">Form add</h6>
                            </div>
                            <div class="card-body">
                                <input type="hidden" name="page" value="1">
                                <div class="row mb-3 form-switch">
                                    <label for="unit" class="form-check-label col-sm-2 text-lg">Unit</label>
                                    <div class="col-sm-10">
                                        <input type="string" class="form-control" name="unit"
                                            value="<%= item.unit %>">
                                    </div>
                                </div>
                                <div class="row mb-3 form-switch">

                                    <label for="name" class="form-check-label col-sm-2 text-lg">Nama</label>
                                    <div class="col-sm-10">
                                        <input type="string" class="form-control" name="name" 
                                            value="<%= item.name %>">
                                    </div>
                                </div>
                                <div class="row mb-3 form-switch">

                                    <label for="note" class="form-check-label col-sm-2 text-lg">Note</label>
                                    <div class="col-sm-10">
                                        <textarea type="string" class="form-control" name="note" 
                                            value=""><%= item.note %></textarea>
                                    </div>
                                </div>
                    
                            </div>
                            <div class="card-footer">
                                <button type="submit" class="btn btn-success" title="Save"><i
                                        class="fas fa-solid fa-database"> Save</i></button>
                                <a type="button" class="btn btn-warning" href="/units" title="Back"><i
                                        class="fas fa-regular fa-hand-point-left"> Back</i></a>
                            </div>
                        </div>
                    </form>
                </div>
        </div>

        <!-- /.container-fluid -->

        <!-- End of Main Content -->
 <!-- Footer -->
 <footer class="sticky-footer bg-white">
